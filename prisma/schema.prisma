
generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Comidas {
  id                     Int @id @default(autoincrement())
  nome                   String
  tipo                   String
  preco                  Float
  descricao              String
  createdAT              DateTime @default(now())
  updateAT                DateTime @updatedAt
  @@map("comidas")

}